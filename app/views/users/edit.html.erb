<%= render  "shared/navbar" %>
<%= link_to "アカウント", users_account_path %>
<%= link_to "プロフィール", users_profile_path %>

<h1>編集</h1>
<%= form_for @user do |f| %>
<table class="new-table">
    <tr>
        <th><%= f.label :avator ,"アイコン画像" %></th>
        <td><%= f.file_field :avator %></td>
    <tr>
        <th><%= f.label :name ,"名前" %></th>
        <td><%= f.text_field :name %></td>
    </tr>
    <tr>
        <th><%= f.label :profile ,"自己紹介" %></th>
        <td><%= f.text_field :profile%></td>
    </tr>
</table>
   
   <div>
       <ul>
           <li><%= link_to "キャンセル", users_profile_path %></li>
           <li><%= f.submit "保存" %></li>
       </ul>
   </div>
<% end %>
<%= render "shared/footer"%>